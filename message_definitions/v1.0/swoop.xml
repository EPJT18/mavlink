      <entry value="33554432‬‬" name="RANGEFINDER_NOT_HEALTHY"/><!-- 25 -->
      <entry value="67108864‬" name="AUTOPILOT_VOLTAGE_ERROR"/><!-- 26 -->
      <entry value="134217728‬‬" name="AUTOPILOT_INTERNAL_SYSTEM_ERROR"/><!-- 27 -->
      <entry value="268435456‬‬" name="CAN_BUS_NOT_HEALTHY"/><!-- 28 -->
      <entry value="536870912‬‬" name="PROXIMITY_ERROR"/> <!-- 29 -->
      <entry value="1073741824‬" name="NO_SD_CARD"/> <!-- 30 -->
