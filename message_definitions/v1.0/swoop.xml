      <entry value="64" name="ACCELS_NOT_CALIBRATED"/> <!-- 6 -->
      <entry value="128" name="ACCELS_INCONSISTENT"/> <!-- 7 -->
      <entry value="256" name="COMPASS_NOT_HEALTHY"/> <!-- 8 -->
      <entry value="512" name="COMPASS_NOT_CALIBRATED"/> <!-- 9 -->
      <entry value="1024" name="COMPASS_INCONSISTENT"/> <!-- 10 -->
      <entry value="2048" name="COMPASS_OFFSETS"/> <!-- 11 -->
      <entry value="4096" name="HIGH_MAG_FIELD_STRENGTH"/> <!-- 12 -->
